<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_paper_test"
    android:layout_width="@dimen/qb_px_1030"
    android:layout_height="@dimen/qb_px_98"
    android:background="@drawable/shape_item_round_white"
    >

    <TextView
        android:id="@+id/tv_paper_chapter"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/dimens_sp_25"
        android:textColor="@color/color_000000"
        android:layout_marginStart="@dimen/qb_px_50"
        android:gravity="center_vertical"
        />

    <TextView
        android:id="@+id/tv_ai_correct"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:text="@string/string_ai_correct"
        android:textSize="@dimen/dimens_sp_25"
        android:textColor="@color/color_0095ff"
        android:gravity="center_vertical"
        android:layout_marginEnd="@dimen/qb_px_50"/>

    <!--<ImageView
        android:id="@+id/iv_icon"
