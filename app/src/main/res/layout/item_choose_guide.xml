<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_paper_test"
    android:layout_width="@dimen/qb_px_100"
    android:layout_height="match_parent"
    android:layout_gravity="center_vertical"
    >

    <TextView
        android:id="@+id/tv_paper_chapter"
        android:layout_width="@dimen/qb_px_40"
        android:layout_height="@dimen/qb_px_40"
        android:textSize="@dimen/dimens_sp_25"
        android:textColor="@color/color_000000"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:background="@drawable/button_circle_shape_pressed"
        />

    <TextView
        android:id="@+id/tv_ai_correct"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/string_ai_correct"
        android:textSize="@dimen/dimens_sp_25"
        android:textColor="@color/color_0095ff"
        android:gravity="center_vertical"
        android:layout_marginEnd="@dimen/qb_px_50"
        android:visibility="gone"/>



</RelativeLayout>